root = true

[*]
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
indent_style = space
indent_size = 2
insert_final_newline = true

[**/.terraform]
ignore = true

[*.md]
trim_trailing_whitespace = false

[*.py]
indent_size = 4

[{Makefile,go.mod,go.sum,*.go,.gitmodules}]
